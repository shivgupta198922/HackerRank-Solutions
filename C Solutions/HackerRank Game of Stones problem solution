#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int n;
    scanf("%d\n",&n);
    int j;
    for (int i =0; i<n;i++){
        scanf("%d\n",&j);
        
        j=j%7;
        if(j<=1){
            printf("Second\n");
        }else{
            printf("First\n");
        }
            
    }
    return 0;
}
